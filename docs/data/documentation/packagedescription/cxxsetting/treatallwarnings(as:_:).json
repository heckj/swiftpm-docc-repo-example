            {
              "inlineContent" : [
                {
                  "text" : "The treatment level for all warnings (",
                  "type" : "text"
                },
                {
                  "code" : ".warning",
                  "type" : "codeVoice"
                },
                {
                  "text" : " or ",
                  "type" : "text"
                },
                {
                  "code" : ".error",
                  "type" : "codeVoice"
                },
                {
                  "text" : ").",
                  "type" : "text"
