        {
          "inlineContent" : [
            {
              "text" : "If you make a remote binary framework available as a Swift package,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "declare a remote, or ",
              "type" : "text"
            },
            {
              "inlineContent" : [
                {
                  "text" : "URL-based",
                  "type" : "text"
                }
              ],
              "type" : "emphasis"
            },
            {
              "text" : ", binary target in your package manifest",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "with ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/org.swift.swiftpm.packagedescription\/documentation\/PackageDescription\/Target\/binaryTarget(name:url:checksum:)",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ". Always run ",
              "type" : "text"
            },
            {
              "code" : "swift package compute-checksum path\/to\/MyFramework.zip",
              "type" : "codeVoice"
            },
            {
              "text" : " at the command line to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "make sure you create a correct SHA256 checksum.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
