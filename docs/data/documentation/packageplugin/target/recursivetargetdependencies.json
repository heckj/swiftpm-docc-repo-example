          "inlineContent" : [
            {
              "text" : "Package manager orders the results such that every dependency appears before any other target that",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "depends on it.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The dependencies are sorted in topological sort order.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
