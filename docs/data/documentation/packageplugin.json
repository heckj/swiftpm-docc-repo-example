  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Build tool plugins generate source files as part of a build, or perform other actions at the start of every build.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "The package manager invokes build tool plugins before a package is built in order to construct command invocations to run as part of the build.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Command plugins provide actions that users can perform at any time and aren’t associated with a build.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Read ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/docs.swift.org\/swiftpm\/documentation\/packagemanagerdocs\/WritingBuildToolPlugin",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " to learn how to create build tool plugins, or ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/docs.swift.org\/swiftpm\/documentation\/packagemanagerdocs\/WritingCommandPlugin",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " to learn how to create command plugins.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
